{
	if(CurrentLevel == NEWWORLD_ZEN)
	{
		B_InitNpc_NW();
	}
	else if(CurrentLevel == ADDONWORLD_ZEN)
	{
		B_InitNpc_ADW();
	}
	else if(CurrentLevel == OLDWORLD_ZEN)
	{
		B_InitNpc_OW();
	}
	else if(CurrentLevel == DRAGONISLAND_ZEN)
	{
		B_InitNpc_DI();
	};
};

